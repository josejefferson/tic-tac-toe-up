{
  "name": "vike-nestjs-template",
  "version": "0.0.1",
  "description": "Vike + NestJS Template",
  "author": {
    "name": "Jefferson Dantas",
    "url": "https://github.com/josejefferson"
  },
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "NODE_ENV=production node dist/main",
    "dev": "nest start --watch",
    "build": "vite build && nest build",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "tsc --noEmit && tsc --project web/tsconfig.json --noEmit",
    "migrate:generate": "typeorm-ts-node-commonjs migration:generate src/migrations/migration -d src/config/orm.config.ts",
    "migrate:run": "typeorm-ts-node-commonjs migration:run -d src/config/orm.config.ts"
  },
