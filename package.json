    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/http-status-codes": "^1.2.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.2.78",
    "@types/uuid": "^9.0.8",
