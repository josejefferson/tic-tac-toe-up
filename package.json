    "multer": "^1.4.5-lts.1",
    "nprogress": "^0.2.0",
    "pg": "^8.11.5",
    "react": "^18.2.0",
    "react-cookie": "^7.1.4",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.51.3",
    "react-icons": "^5.1.0",
